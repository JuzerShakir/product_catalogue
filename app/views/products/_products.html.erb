<%= turbo_frame_tag "products" do %>
        <% @products.each do |product| %>
            <p>*Name: <%= product.name %> </p>
            <p>UPC: <%= product.upc %> </p>
            <p>Available On: <%= product.available_on.strftime("%d/%m/%Y")  %> </p>

            Properties:( <%= properties_listing_of product %> )
            <p>----</p>
        <% end %>
<% end %>